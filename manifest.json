{
  "manifest_version": 2,

  "name": "Personal Bookmark",
  "description": "A extension send bookmark to personal server",
  "version": "1.0",
  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*"
  ]
}
